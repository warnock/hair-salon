<!--stylist-form.vtl-->
<h2>New Stylist</h2>
<form action="/stylists" method="post">
  <div class="form-group">
    <label for="name">Name</label>
    <input id="name" name="name" type="text" class="form-control" required>
  </div>
  <button type="submit" class="btn btn-success">Add Stylist!</button>
</form>

